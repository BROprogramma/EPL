                     type="eplcom:AmendmentTypeType">
            <xs:annotation>
               <xs:appinfo>
                  <CodeListName>AmendmentType</CodeListName>
                  <CodeListURI/>
               </xs:appinfo>
               <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De typering van de wijziging in de vergunning.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
