    <xs:complexType name="AmendmentAreaType">
        <xs:sequence>
            <xs:element name="areaToBeRenounced" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het gebiedsnummer van het verleende gebied dat zal worden vervangen door het op te splitsen in 2 of meer gebieden, zodra de wijziging ingaat.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="1"/>
                    </xs:restriction>
                </xs:simpleType>
